<% provide :page_title, "Restaurants" %>

<%= link_to "Create a new restaurant", new_restaurant_path %>

<h1>Restaurants</h1>

<% @restaurants.each do |restaurant|%>

	<h3><%= link_to restaurant.name.capitalize, restaurant_path(restaurant.id) %> </h3>
	<p><%= restaurant.description %> </p>

	<% end %>